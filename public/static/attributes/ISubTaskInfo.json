{
  "classId": "idm.componet.infoAction.isubTaskInfo",
  "comName": "子任务信息",
  "className": "ISubTaskInfo",
  "comType": "common",
  "comLangue": "vue",
  "compositeAttr": [
      {
          "type": "input",
          "layoutType": "inline",
          "text": "唯一标识",
          "bindKey": "ctrlId",
          "disabled": true,
          "default": "@[packageid]",
          "desc": ""
      },
      {
        "type": "group",
        "text": "基本属性",
        "desc": "提供设置组件能达到展示效果的基本属性设置信息",
        "children": [
          {
            "type": "inputNumber",
            "layoutType": "inline",
            "text": "图片样式",
            "bindKey": "emptySize",
            "desc":"暂无数据图片高度：100",
            "default": 100
          }
        ]
      },
      {
        "type": "group",
        "text": "列表样式",
        "desc": "点击？查看样式设置用法指南",
        "helpUrl": "",
        "children": [
          {
            "type": "box",
            "layoutType": "block",
            "text": "内外边距",
            "bindKey": "libox",
            "default": {
              "marginTopVal": "",
              "marginRightVal": "",
              "marginBottomVal": "",
              "marginLeftVal": "",
              "paddingTopVal": "14px",
              "paddingRightVal": "",
              "paddingBottomVal": "10px",
              "paddingLeftVal": ""
            }
          },
          {
            "type": "border",
            "layoutType": "block",
            "text": "边框",
            "bindKey": "liBoxborder",
            "default": {
              "border": {
                "top": {
                    "style": "dashed",
                    "width": 1,
                    "widthUnit": "px",
                    "colors": {
                      "hex8": "#ccc"
                    }
                },
                "right": {
                    "style": "solid",
                    "width": 0,
                    "widthUnit": "px",
                    "colors": {
                      "hex8": ""
                    }
                },
                "bottom": {
                    "style": "solid",
                    "width": 0,
                    "widthUnit": "px",
                    "colors": {
                      "hex8": ""
                    }
                },
                "left": {
                    "style": "solid",
                    "width": 0,
                    "widthUnit": "px",
                    "colors": {
                      "hex8": ""
                    }
                }
            },
              "radius": {
                  "leftTop": {
                      "radius": 0,
                      "radiusUnit": "px"
                  },
                  "rightTop": {
                      "radius": 0,
                      "radiusUnit": "px"
                  },
                  "leftBottom": {
                      "radius": 0,
                      "radiusUnit": "px"
                  },
                  "rightBottom": {
                      "radius": 0,
                      "radiusUnit": "px"
                  }
              }
            }
          }
        ]
      },
      {
        "type": "group",
        "text": "外层样式",
        "desc": "点击？查看样式设置用法指南",
        "helpUrl": "",
        "children": [
            {
              "type": "inlineGroup",
              "text": "宽高",
              "children": [
                {
                  "type": "input",
                  "layoutType": "block",
                  "text": "宽",
                  "desc": "填写auto则为自适应，或者使用px、%、vw等单位，比如100%、100px、100vw等等",
                  "bindKey": "width",
                  "width": "60px",
                  "default": "100%"
                },
                {
                  "type": "input",
                  "layoutType": "block",
                  "text": "高",
                  "desc": "填写auto则为自适应，或者使用px、%、vh等单位，比如100%、100px、100vh等等",
                  "bindKey": "height",
                  "width": "60px",
                  "default": "100%"
                }
              ]
            },
            {
              "type": "box",
              "layoutType": "block",
              "text": "内外边距",
              "bindKey": "ulbox"
            },
            {
              "type": "colorPicker",
              "layoutType": "inline",
              "text": "背景色",
              "bindKey": "bgColor",
              "placeholder": "请选择颜色",
              "default": {
                  "hex": "#ffffff",
                  "hex8": "#ffffff"
              }
            },
            {
              "type": "input",
              "layoutType": "inline",
              "text": "容器阴影",
              "bindKey": "boxShadow",
              "default": ""
            },
            {
              "type": "border",
              "layoutType": "block",
              "text": "边框",
              "bindKey": "boxborder"
            }
        ]
      },
      {
        "type": "table",
        "text": "主题设置",
        "desc": "点击？查看主题设置用法指南",
        "bindKey": "themeList",
        "tabText": "主题[index]",
        "default": [
          {
            "key": "blue",
            "mainColor": {
              "hex": "#0073CA",
              "hex8": "#0073CAFF"
            }
          },
          {
            "key": "red",
            "mainColor": {
              "hex": "#E21A1A",
              "hex8": "#E21A1AFF"
            }
          },
          {
            "key": "green",
            "mainColor": {
              "hex": "#0EAF64",
              "hex8": "#0EAF64FF"
            }
          }
        ],
        "children": [
          {
            "type": "select",
            "layoutType": "inline",
            "text": "主题标识",
            "bindKey": "key",
            "desc": "用于设置主题的标识，此项需要搭配自定义开发切换主题的功能，例如：蓝色标识为blue，用户切换主题的时候存储到数据库中的标识为blue，则这里填写为blue",
            "default": "",
            "placeholder": "例如：blue",
            "ctrlAttrObject": {
              "mode": "combobox"
            },
            "dictionary": [
              {
                "label": "blue",
                "value": "blue"
              },
              {
                "label": "red",
                "value": "red"
              },
              {
                "label": "green",
                "value": "green"
              }
            ]
          },
          {
            "type": "colorPicker",
            "layoutType": "inline",
            "text": "主要颜色",
            "bindKey": "mainColor",
            "desc": "主题的主要颜色",
            "placeholder": "请选择颜色",
            "display": "@[key]"
          }
        ]
      },
      {
        "type": "group",
        "text": "高级",
        "desc": "点击？查看样式设置用法指南",
        "helpUrl": "",
        "children": [
          {
            "type": "actionSelect",
            "layoutType": "block",
            "text": "子项跳转函数",
            "bindKey": "handleChildFunc",
            "desc": "接口参数：自定义函数参数：{_this: this, item: item}",
            "multiple": false
          },
          {
            "type": "actionSelect",
            "layoutType": "block",
            "text": "附件跳转函数",
            "bindKey": "handleFileFunc",
            "desc": "接口参数：自定义函数参数：{_this: this, item: item}",
            "multiple": false
          }
        ]
      }
  ]
}