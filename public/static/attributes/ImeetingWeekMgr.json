{
  "classId": "idm.componet.meeting.imeetingweekmgr",
  "comName": "会议室管理",
  "className": "ImeetingWeekMgr",
  "comType": "common",
  "comLangue": "vue",
  "compositeAttr": [
      {
          "type": "input",
          "layoutType": "inline",
          "text": "唯一标识",
          "bindKey": "ctrlId",
          "disabled": true,
          "default": "@[packageid]",
          "desc": ""
      },
      {
        "type": "group",
        "text": "基本属性",
        "desc": "提供设置组件能达到展示效果的基本属性设置信息",
        "children": [
          {
            "type": "input",
            "layoutType": "inline",
            "text": "表格高度",
            "desc": "表格高度",
            "bindKey": "tableHeight",
            "default": "calc(100vh - 90px)"
          },
          {
            "type": "input",
            "layoutType": "inline",
            "text": "表格内容",
            "desc": "表格内容",
            "bindKey": "tableContent",
            "default": "calc(100vh - 179px)"
          },
          {
            "type": "input",
            "layoutType": "inline",
            "text": "抽屉宽度",
            "desc": "抽屉宽度",
            "bindKey": "drawerWidth",
            "default": "30%"
          },
          {
            "type": "input",
            "layoutType": "inline",
            "text": "抽屉iframe",
            "desc": "抽屉内容的iframe高度",
            "bindKey": "drawerIframeHeight",
            "default": "100%"
          },
          {
            "type": "box",
            "layoutType": "block",
            "text": "iframe边距",
            "bindKey": "iframeUlBox",
            "default": {
              "marginTopVal": "",
              "marginRightVal": "",
              "marginBottomVal": "",
              "marginLeftVal": "",
              "paddingTopVal": "0",
              "paddingRightVal": "",
              "paddingBottomVal": "",
              "paddingLeftVal": ""
            }
          }
        ]
      },
      {
        "type": "group",
        "text": "外层样式",
        "desc": "点击？查看样式设置用法指南",
        "helpUrl": "",
        "children": [
            {
              "type": "inlineGroup",
              "text": "宽高",
              "children": [
                {
                  "type": "input",
                  "layoutType": "block",
                  "text": "宽",
                  "desc": "填写auto则为自适应，或者使用px、%、vw等单位，比如100%、100px、100vw等等",
                  "bindKey": "width",
                  "width": "60px",
                  "default": "100%"
                },
                {
                  "type": "input",
                  "layoutType": "block",
                  "text": "高",
                  "desc": "填写auto则为自适应，或者使用px、%、vh等单位，比如100%、100px、100vh等等",
                  "bindKey": "height",
                  "width": "60px",
                  "default": "100%"
                }
              ]
            },
            {
              "type": "box",
              "layoutType": "block",
              "text": "内外边距",
              "bindKey": "ulbox",
              "default": {
                "marginTopVal": "",
                "marginRightVal": "",
                "marginBottomVal": "",
                "marginLeftVal": "",
                "paddingTopVal": "20px",
                "paddingRightVal": "20px",
                "paddingBottomVal": "10px",
                "paddingLeftVal": "20px"
              }
            },
            {
              "type": "colorPicker",
              "layoutType": "inline",
              "text": "背景色",
              "bindKey": "bgColor",
              "placeholder": "请选择颜色",
              "default": {
                  "hex": "#ffffff",
                  "hex8": "#ffffff"
              }
            },
            {
              "type": "input",
              "layoutType": "inline",
              "text": "容器阴影",
              "bindKey": "boxShadow",
              "default": ""
            },
            {
              "type": "border",
              "layoutType": "block",
              "text": "边框",
              "bindKey": "boxborder"
            }
        ]
      },
      {
        "type": "group",
        "text": "高级",
        "desc": "点击？查看样式设置用法指南",
        "helpUrl": "",
        "children": [
          {
            "type": "actionSelect",
            "layoutType": "block",
            "text": "会议室申请",
            "bindKey": "handleJump",
            "desc": "接口参数：自定义函数参数：{_this: this, data: 接口数据}",
            "multiple": false
          },
          {
            "type": "actionSelect",
            "layoutType": "block",
            "text": "会议申请",
            "bindKey": "handleMeetingJump",
            "desc": "接口参数：自定义函数参数：{_this: this, data: 当条数据}",
            "multiple": false
          },
          {
            "type": "actionSelect",
            "layoutType": "block",
            "text": "会议块申请",
            "bindKey": "handleMeetingBlockJump",
            "desc": "接口参数：自定义函数参数：{_this: this, data: 当条数据}",
            "multiple": false
          }
        ]
      }
  ]
}