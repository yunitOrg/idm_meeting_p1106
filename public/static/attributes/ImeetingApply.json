{
  "classId": "idm.componet.meeting.imeetingapply",
  "comName": "会议申请",
  "className": "ImeetingApply",
  "comType": "common",
  "comLangue": "vue",
  "compositeAttr": [
      {
          "type": "input",
          "layoutType": "inline",
          "text": "唯一标识",
          "bindKey": "ctrlId",
          "disabled": true,
          "default": "@[packageid]",
          "desc": ""
      },
      {
        "type": "group",
        "text": "基本属性",
        "desc": "提供设置组件能达到展示效果的基本属性设置信息",
        "children": [
          {
            "type": "switch",
            "layoutType": "inline",
            "text": "显示完整房间名称",
            "desc": "开启后将显示完整房间名称，关闭将切割至7个字符",
            "bindKey": "showAllRoomName",
            "default": false
          },
          {
            "type": "switch",
            "layoutType": "inline",
            "text": "关闭时间限制",
            "desc": "开启将可以任意创建，关闭将启用时间控制不允许创建",
            "bindKey": "timeControl",
            "default": false
          },
          {
            "type": "input",
            "layoutType": "inline",
            "text": "日查看",
            "desc": "日查看高度",
            "bindKey": "meetingDayHei",
            "default": "calc(100vh - 230px)"
          },
          {
            "type": "input",
            "layoutType": "inline",
            "text": "周查看",
            "desc": "周查看高度",
            "bindKey": "meetingWeekHei",
            "default": "calc(100vh - 200px)"
          },
          {
            "type": "input",
            "layoutType": "inline",
            "text": "会议表格",
            "desc": "会议表格高度",
            "bindKey": "meetingTableHeight",
            "default": "calc(100vh - 480px)"
          },
          {
            "type": "input",
            "layoutType": "inline",
            "text": "iframe高",
            "desc": "iframe高度",
            "bindKey": "iframeHeight",
            "default": "calc(100vh - 80px)"
          },
          {
            "type": "input",
            "layoutType": "inline",
            "text": "图例bottom",
            "desc": "图例距离底部距离",
            "bindKey": "legendBottomDis",
            "default": "5px"
          }
        ]
      },
      {
        "type": "group",
        "text": "外层样式",
        "desc": "点击？查看样式设置用法指南",
        "helpUrl": "",
        "children": [
            {
              "type": "inlineGroup",
              "text": "宽高",
              "children": [
                {
                  "type": "input",
                  "layoutType": "block",
                  "text": "宽",
                  "desc": "填写auto则为自适应，或者使用px、%、vw等单位，比如100%、100px、100vw等等",
                  "bindKey": "width",
                  "width": "60px",
                  "default": "100%"
                },
                {
                  "type": "input",
                  "layoutType": "block",
                  "text": "高",
                  "desc": "填写auto则为自适应，或者使用px、%、vh等单位，比如100%、100px、100vh等等",
                  "bindKey": "height",
                  "width": "60px",
                  "default": "100%"
                }
              ]
            },
            {
              "type": "box",
              "layoutType": "block",
              "text": "内外边距",
              "bindKey": "ulbox"
            },
            {
              "type": "colorPicker",
              "layoutType": "inline",
              "text": "背景色",
              "bindKey": "bgColor",
              "placeholder": "请选择颜色",
              "default": {
                  "hex": "#ffffff",
                  "hex8": "#ffffff"
              }
            },
            {
              "type": "input",
              "layoutType": "inline",
              "text": "容器阴影",
              "bindKey": "boxShadow",
              "default": ""
            },
            {
              "type": "border",
              "layoutType": "block",
              "text": "边框",
              "bindKey": "boxborder"
            }
        ]
      },
      {
        "type": "group",
        "text": "高级",
        "desc": "点击？查看样式设置用法指南",
        "helpUrl": "",
        "children": [
          {
            "type": "input",
            "layoutType": "block",
            "text": "设置周查看晚上时间",
            "bindKey": "WeekNightTime",
            "desc": "事件格式为：18:00 (HH:mm), 默认为18:00",
            "default": "18:00"
          },
          {
            "type": "actionSelect",
            "layoutType": "block",
            "text": "我的申请链接",
            "bindKey": "handleUrlFunc",
            "desc": "接口参数：自定义函数参数：{_this: this}",
            "multiple": false
          },
          {
            "type": "actionSelect",
            "layoutType": "block",
            "text": "色块跳转链接",
            "bindKey": "handleColorJumpUrl",
            "desc": "接口参数：自定义函数参数：{_this: this, item: item}",
            "multiple": false
          },
          {
            "type": "actionSelect",
            "layoutType": "block",
            "text": "色块鼠标悬停提示内容",
            "bindKey": "handleTipsContentFunc",
            "desc": "接口参数：自定义函数参数：{_this: this, item: item}",
            "multiple": false
          },
          {
            "type": "actionSelect",
            "layoutType": "block",
            "text": "色块鼠标悬停提示锁定内容",
            "bindKey": "handleTipsContentLockFunc",
            "desc": "接口参数：自定义函数参数：{_this: this, item: item}",
            "multiple": false
          },
          {
            "type": "actionSelect",
            "layoutType": "block",
            "text": "按会议室申请楼详情",
            "bindKey": "handleMeetingRoomDetail",
            "desc": "接口参数：自定义函数参数：{_this: this, data: data}",
            "multiple": false
          }
        ]
      }
  ]
}